{
    "name": "kudaiberdy/node_diff_generator",
    "description": "this package defines the difference between two files json, yaml format",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Kudaiberdy",
            "email": "kudaiberdy.shagirov@gmail.com"
        }
    ],
    "autoload": {
